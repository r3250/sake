<div class="container">
  <div class="row">
  	<div class="col-lg-2">
  	</div>
  	<div class="col-lg-10">
  	<% @user_posts.each do |user_post| %>
  	<%= link_to user_path(user_post.user_id) do %>
  	 <% 10.times do %>
  	  <%= attachment_image_tag user_post, :image, :fill, 200, 300, format: 'jpg' %>
  	  <% end %>
  	<% end %>
  	  <% end %>
  	</div>
  	<div class="paginate">
    <%= paginate @user_posts %>
    </div>
  </div>
</div>